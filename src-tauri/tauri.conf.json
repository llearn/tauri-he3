{
  "build": {
    "devPath": "../he3-dist",
    "distDir": "../he3-dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "tauri-he3",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.ccm.he3",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://releases.myapp.com/{{target}}/{{current_version}}"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDlCQjg2QkY3N0REMTkyNzAKUldSd2t0Rjk5MnU0bTdVdUkzMi9lY1h5TXAxUUtyK201dVJGbExybjJ5aVF2ejNXaFQvODhuSkYK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tauri-he3",
        "width": 1000,
        "height": 600
      }
    ]
  }
}